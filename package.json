{
  "name": "Stride-Actions",
  "version": "0.0.1",
  "description": "I'm an example of Stride actions.",
  "author": "Sheldon Callahan <sheldon@atlassian.com>",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "lint": "eslint . --ext .js --fix",
    "depcheck": "depcheck",
    "ngrok": "ngrok http 8000 --log=stdout | npm start"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "morgan": "^1.9.0",
    "prettyjson": "^1.2.1",
    "pug": "*",
    "request": "^2.72.0"
  },
  "repository": {
    "url": "git@bitbucket.org:atlassian/stride-action-example.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "node",
    "glitch",
    "express",
    "stride",
    "atlassian",
    "bot"
  ],
  "devDependencies": {
    "nodemon": "*",
    "depcheck": "*",
    "eslint": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-standard": "*",
    "prettier": "*"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "plugins": [
      "standard",
      "prettier"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    }
  }
}
